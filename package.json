    },
    "homepage": "https://github.com/julyeli/WebDriverIO-Mobile#readme",
    "devDependencies": {
        "@wdio/allure-reporter": "^7.19.0",
        "@wdio/appium-service": "^7.19.5",
        "@wdio/cli": "^7.19.7",
        "@wdio/cucumber-framework": "^7.19.7",
        "@wdio/local-runner": "^7.19.7",
        "@wdio/selenium-standalone-service": "^7.19.5",
        "@wdio/spec-reporter": "^7.19.7",
        "appium": "^1.22.3",
