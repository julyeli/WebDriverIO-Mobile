{
    "name": "wdio-cucumber",
    "version": "1.0.0",
    "description": "framework webdriverio cucumber js to mobile",
    "main": "index.js",
    "scripts": {
        "test": "test",
        "wdio": "wdio run wdio.conf.js",
        "code:prettier": "prettier --check \"**/*.js*\"",
        "report:allure": "npx allure",
